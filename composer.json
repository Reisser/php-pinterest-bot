{
  "name":  "seregazhuk/pinterest-bot",
    "description": "PHP library for Pintrest",
	"keywords": [
		"php",
		"lib",
		"pinterest"
	],
	"type": "package",
	"minimum-stability": "dev",
	"license": "Mit",
	"autoload": {
		"psr-4": {
            "seregazhuk\\PinterestBot\\": "src"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "seregazhuk\\PinterestBot\\": "src",
            "seregazhuk\\tests\\": "tests"
        }
    },
	"repositories": [
		{
			"type": "vcs",
            "url": "git@github.com:seregazhuk/php-pinterest-bot.git"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"ext-curl": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "*",
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "*"
	},
	"authors": [
		{
			"name": "Serega Zhuk",
			"email": "seregazhuk88@gmail.com",
			"role": "Developer"
		}
	]
}
